
@{
    ViewData["Title"] = "Callbycsharp";
}
<h1>Call Api synchronous</h1>
<table class="table">
    <thead>
        <tr>

            <th scope="col">SupplierID</th>
            <th scope="col">Rate</th>

        </tr>
    </thead>
    <tbody>
        @{
            foreach (var t in Model.GetApi)
            {
                <tr>
                    <td>@t.SupplierID</td>
                    <td>@t.Rate</td>

                </tr>
            }
        }


    </tbody>
</table>
<h1>Call Api asynchronous</h1>
<table class="table">
    <thead>
        <tr>
          
            <th scope="col">SupplierID</th>
            <th scope="col">Rate</th>
            
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var t in Model.GetAsync)
            {
            <tr>
               
                
                <td>@t.SupplierID</td>
                <td>@t.Rate</td>
               
            </tr>
                }
            }


    </tbody>
</table>



